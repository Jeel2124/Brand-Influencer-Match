export default async function handler(req, res) {
  if (req.method === 'POST') {
    const influencers = req.body; // Zapier will send JSON here
    // TODO: Save influencers data somewhere (database, cache, etc.)

    // For now just send back success response
    res.status(200).json({ message: 'Data received', data: influencers });
  } else {
    res.status(405).json({ message: 'Method Not Allowed' });
  }
}
